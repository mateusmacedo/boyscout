{
  "rabbit_version": "3.12.0",
  "rabbitmq_version": "3.12.0",
  "users": [
    {
      "name": "boyscout",
      "password_hash": "boyscout123",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "boyscout"
    }
  ],
  "permissions": [
    {
      "user": "boyscout",
      "vhost": "boyscout",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "exchanges": [
    {
      "name": "boyscout.notifications",
      "vhost": "boyscout",
      "type": "topic",
      "durable": true,
      "auto_delete": false
    },
    {
      "name": "boyscout.events",
      "vhost": "boyscout",
      "type": "topic",
      "durable": true,
      "auto_delete": false
    }
  ],
  "queues": [
    {
      "name": "notifications.email",
      "vhost": "boyscout",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000
      }
    },
    {
      "name": "notifications.sms",
      "vhost": "boyscout",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 3600000
      }
    },
    {
      "name": "events.audit",
      "vhost": "boyscout",
      "durable": true,
      "auto_delete": false
    }
  ],
  "bindings": [
    {
      "source": "boyscout.notifications",
      "vhost": "boyscout",
      "destination": "notifications.email",
      "destination_type": "queue",
      "routing_key": "email.*"
    },
    {
      "source": "boyscout.notifications",
      "vhost": "boyscout",
      "destination": "notifications.sms",
      "destination_type": "queue",
      "routing_key": "sms.*"
    },
    {
      "source": "boyscout.events",
      "vhost": "boyscout",
      "destination": "events.audit",
      "destination_type": "queue",
      "routing_key": "audit.*"
    }
  ]
}
